<h1>Picturehash#index</h1>
<p>Find me in app/views/picturehash/index.html.erb</p>
<div><%#= @asdf %></div>
<div>Available: <%= @asdf.count %></div>
<div>In ActiveStorage: <%= @pictures_count %></div>
<% if @pictures_count > 50 %>
  <%== pagy_bootstrap_nav(@pagy) %>
  <% pic = @records %>
<% else %>
  <% pic = @pictures %>
<% end %>
<%= "===========================================" %>
<% 2.times { %><br>
<% } %>
<div class="container">
  <div class="row">
    <% pic.each do |aaa| %>
      <div class="col-sm m-1 text-center">
        <div><%= link_to image_tag(aaa.variant(resize_to_limit: [100, 100])), picture_path(aaa) %></div>
        <div><%= link_to aaa.id, picture_path(aaa) %></div>
      </div>
    <% end %>
  </div>
</div>
<%# picture_hash_show_path %>
<% 10.times { %><br>
<% } %>
